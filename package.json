{
  "name": "audiophile-api",
  "version": "1.0.0",
  "description": "REST API created with Express, Sequelize, Docker and PostgreSQL",
  "main": "src/app.js",
  "scripts": {
    "dev": "nodemon src/server.js",
    "migrations:generate": "sequelize-cli migration:generate --name",
    "migrations:run": "sequelize-cli db:migrate",
    "migrations:revert": "sequelize-cli db:migrate:undo",
    "migrations:delete": "sequelize-cli db:migrate:undo:all"
  },
  "keywords": [
    "REST API",
    "Frontend Mentor",
    "audiophile-api"
  ],
  "author": "BranLeeDev",
  "license": "MIT",
  "devDependencies": {
    "eslint": "^8.51.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "sequelize-cli": "^6.6.1"
  },
  "dependencies": {
    "@hapi/boom": "^10.0.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "joi": "^17.11.0",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.33.0"
  }
}
